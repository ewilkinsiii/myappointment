
  <%#= month_calendar events: @appointments do |date, appointments| %>
  <%=month_calendar events: @appointments do |date, appointments| %>
    <%= date.strftime("%d") %>
    
    <% appointments.each do |appointment| %>
    <div>
      <span class="badge badge-primary"><%= appointment.name%>  <%=appointment.start_time.strftime("%l:%M%p")%></span>
    </div>
  <% end %>
  <% end %>
  
  <%= week_calendar number_of_weeks: 2 do |date| %>
    <%= date.strftime("%d") %>
  <% end %>
  
  <%= calendar number_of_days: 4 do |date| %>
    <%= date.strftime("%d")%>
  <% end %>
  
  <%= calendar start_date_param: :my_date do |date| %>
    <%= date.strftime("%d") %>
  <% end %>